# appl/test/threadtests/Imakefile

/* makedepend generates dependency rules for 
      C   files listed in $(SRCS) and
      C++ files listed in $(CXXSRCS) 
   Don't put C++ files in $(SRCS)
*/
SRCS    =
CXXSRCS = threadcancel.cc diner.cc

INCLUDES = $(OMNITHREADDEFINE)


all:: diner

NormalCplusplusProgramTarget(diner,diner.o,$(OMNITHREADDEPEND),$(OMNITHREADLIB),$(OMNIEXTRASYSLIB))

InstallProgram(diner,$(BINDIR))

all:: threadcancel

NormalCplusplusProgramTarget(threadcancel,threadcancel.o,$(OMNITHREADDEPEND),$(OMNITHREADLIB),$(OMNIEXTRASYSLIB))

InstallProgram(threadcancel,$(BINDIR))

DependTarget()
